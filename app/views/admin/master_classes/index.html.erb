<div id="master_classes">
  <h1><%= t ".title" %></h1>
  <div>
    <div class="class-form col-md-10 col-md-offset-1">
      <%= render "button_create" %>
    </div>
    <div class="col-md-10 col-md-offset-1">
      <div class="search-form">
        <%= render "search_form", search: @search %>
      </div>
      <div id="class-main">
        <%= render "master_classes_table", search: @search,
          master_classes: @master_classes %>
      </div>
    </div>
  </div>
</div>
<script type="text/javascript">
  $(document).on('turbolinks:load ajaxComplete', function() {
    $(".cancel").on("click", function (argumen) {
      $(".class-form").html("<%= j render 'button_create' %>");
      $("tr.tr-edit").removeClass("tr-edit");
    })
  });
</script>

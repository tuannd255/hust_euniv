<tr class="master-class master-class-<%= master_class.id %>"
  data-id="<%= master_class.id %>">
  <td><%= master_class.code %></td>
  <td><%= master_class.name %></td>
  <td><%= link_to t("buttons.edit"), edit_admin_master_class_path(master_class),
    remote: true, class: "edit-master-class" %></td>
  <td><%= link_to t("buttons.delete"), admin_master_class_path(master_class),
    method: :delete, remote: true, data: {confirm: t("confirm")} %></td>
</tr>

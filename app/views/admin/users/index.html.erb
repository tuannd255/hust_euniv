<div id="users">
  <h1><%= t ".title"%></h1>
  <div>
    <div class="user-form col-md-10 col-md-offset-1">
      <%= render "button_create" %>
    </div>
    <div class="col-md-10 col-md-offset-1">
      <div class="search-form">
        <%= render "search_form", support: @support %>
      </div>
      <div id="user-main">
        <%= render "user_table", search: @support.search,
          users: @users %>
      </div>
    </div>
  </div>
</div>
<script type="text/javascript">
  $(document).on('turbolinks:load ajaxComplete', function() {
    $(".cancel").on("click", function (argumen) {
      $(".user-form").html("<%= j render 'button_create' %>");
    })
  });
</script>

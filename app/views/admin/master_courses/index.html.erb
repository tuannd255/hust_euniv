<div id="master_courses">
  <h1><%= t ".title" %></h1>

  <div>
    <div class="course-form col-md-10 col-md-offset-1">
      <%= render "button_create" %>
    </div>
    <div class="col-md-10 col-md-offset-1">
      <div class="search-form">
        <%= render "search_form", search: @search %>
      </div>
      <div id="course-main">
        <%= render "master_courses_table", search: @search, master_courses: @master_courses %>
      </div>
    </div>
  </div>
</div>
<script type="text/javascript">
  $(document).on('turbolinks:load ajaxComplete', function() {
    $(".cancel").on("click", function (argumen) {
      $(".course-form").html("<%= j render 'button_create' %>");
      $("tr.tr-edit").removeClass("tr-edit");
    })
  });
</script>

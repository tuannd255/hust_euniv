<div id="positions">
  <h1><%= t ".title" %></h1>
  <div>
    <div class="position-form col-md-10 col-md-offset-1">
      <%= render "button_create" %>
    </div>
    <div class="col-md-10 col-md-offset-1">
      <div class="search-form">
        <%= render "search_form", search: @support.search %>
      </div>
      <div id="position-main">
        <%= render "positions_table", search: @support.search,
          positions: @positions %>
      </div>
    </div>
  </div>
</div>
<script type="text/javascript">
  $(document).on('turbolinks:load ajaxComplete', function() {
    $(".cancel").on("click", function (argumen) {
      argumen.preventDefault();
      $(".position-form").html("<%= j render 'button_create' %>");
      $("tr.tr-edit").removeClass("tr-edit");
    })
  });
</script>

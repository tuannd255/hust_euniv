<tr class="master-subject-<%= master_subject.id %>">
  <td><%= master_subject.code %></td>
  <td><%= master_subject.name %></td>
  <td><%= master_subject.creadit %></td>
  <td class="icon-link text-center">
    <%= link_to edit_admin_master_subject_path(master_subject),
      remote: true, class: "edit-master-subject" do %>
      <i class="fa fa-pencil-square-o index-button" aria-hidden="true"
        title="<%= t 'buttons.edit'%>"></i>
    <% end %>
    <%= link_to admin_master_subject_path(master_subject),
      method: :delete, data: {confirm: t("confirm")} do %>
      <i class="fa fa-times index-button" aria-hidden="true" title="<%= t 'buttons.delete'%>"></i>
    <% end %>
  </td>
</tr>

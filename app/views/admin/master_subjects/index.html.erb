<div id="master_subjects">
  <h1>Admin::MasterSubject#index</h1>
  <div>
    <div class="subject-create col-md-10 col-md-offset-1">
      <%= form_for [:admin, @master_subject], remote: true do |f| %>
        <%= render "shared/error_messages", object: @master_subject %>
        <div class="form-group">
          <%= f.label :code, t(".code") %>
          <%= f.text_field :code, class: "form-control",
            placeholder: t(".please_input_code") %>
        </div>

        <div class="form-group">
          <%= f.label :name, t(".name") %>
          <%= f.text_field :name, class: "form-control",
            placeholder: t(".please_input_name") %>
        </div>

        <div class="form-group">
          <%= f.label :creadit, t(".creadit") %>
          <%= f.number_field :creadit, class: "form-control",
            placeholder: t(".please_input_creadit") %>
        </div>

        <%= f.submit t("buttons.create"), class: "btn btn-primary" %>
      <% end %>
    </div>
    <div class="col-md-10 col-md-offset-1">
      <div>
        <%= render "search_form", search: @search %>
      </div>
      <div id="subject-main">
        <%= render "master_subjects_table", search: @search,
          master_subjects: @master_subjects %>
      </div>
    </div>
  </div>
</div>

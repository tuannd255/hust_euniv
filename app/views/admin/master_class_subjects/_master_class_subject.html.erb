<tr class="master-class-subject master-class-subject-<%= master_class_subject.id %>">
  <td>
    <%= master_class_subject.code %>
  </td>
  <td class="truncate-col" title="<%= master_class_subject.master_subject_name %>">
    <%= master_class_subject.master_subject_code %>
  </td>
  <td class="truncate-col" title="<%= master_class_subject.name %>">
    <%= master_class_subject.name %>
  </td>
  <td class="truncate-col" title="<%= master_class_subject.master_class_name %>">
    <%= master_class_subject.master_class_name %>
  </td>
  <td class="truncate-col" title="<%= master_class_subject.user_name %>">
    <%= master_class_subject.user_name %>
  </td>
  <td>
    <%= master_class_subject.room_name %>
  </td>

  <td class="text-center">
    <%= link_to edit_admin_master_class_subject_path(master_class_subject),
      remote: true, class: "edit-master-class-subject" do %>
      <i class="fa fa-pencil-square-o index-button" aria-hidden="true"
        title="<%=t 'buttons.edit'%>"></i>
    <% end %>
    <%= link_to admin_master_class_subject_path(master_class_subject),
      method: :delete, data: {confirm: t("confirm")} do %>
      <i class="fa fa-times index-button" aria-hidden="true"
        title="<%= t 'buttons.delete' %>"></i>
    <% end %>
  </td>
</tr>
